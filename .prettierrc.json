{
  "$schema": "https://json.schemastore.org/prettierrc",
  "arrowParens": "always",
  "endOfLine": "auto",
  "printWidth": 100,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],  
  "importOrder": [
    "^vue(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/views/(.*)$",
    "^@/store/(.*)$",
    "^@/(.*)/(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
